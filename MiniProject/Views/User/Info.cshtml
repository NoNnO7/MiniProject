@model User;
@{
    ViewData["Title"] = "Info User";
}
<h1 style="text-align: center;">Info User</h1>
<form method="POST" asp-controller="Order" asp-action="Privacy">
    <div class="form-group">
        <label asp-for="Id"></label>
        <input type="text" asp-for="Id" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Menu"></label>
        <input type="text" asp-for="Menu" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Option"></label>
        <input type="text" asp-for="Option" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Location"></label>
        <input type="text" asp-for="Location" class="form-control" />
    </div>
    <p></p>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>